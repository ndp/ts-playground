{
  "name": "ts-service-worker",
  "version": "1.0.9",
  "description": "",
  "engines": {
    "node": ">=16"
  },
  "type": "commonjs",
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./types/index.d.ts",
  "files": [
    "README.md",
    "dist",
    "types"
  ],
  "scripts": {
    "example1": "node --loader tsc example/amp-what-sw.mts",
    "example2": "node --loader tsc example/offline-backup.mts",
    "test": "node --test --loader tsx src/specs/*.mts",
    "build": "yarn clean && yarn build-dist && yarn build-types",
    "clean": "rm -fr ./dist/* ./types/*",
    "build-dist": "./node_modules/.bin/esbuild --format=esm  --target=esnext --outfile=./dist/index.js --bundle --sourcemap --platform=node --minify --format=cjs src/index.mts && cp ./src/serviceWorker.template.js ./dist/",
    "build-types": "tsc -p src/tsconfig.json",
    "prepublishOnly": "yarn build"
  },
  "keywords": [],
  "author": "Andrew Peterson",
  "license": "ISC",
  "devDependencies": {
    "esbuild": "^0.19.4",
    "tsx": "^3.13.0"
  }
}
